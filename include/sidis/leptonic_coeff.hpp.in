			["Real", "theta_1"],
			["Real", "theta_2"],
			["Real", "theta_3"],
			["Real", "theta_4"],
		],
		("U", "P"): [
			["Real", "theta_6"],
			["Real", "theta_8"],
		],
		("L", "U"): [
			["Real", "theta_5"],
		],
		("L", "P"): [
			["Real", "theta_7"],
			["Real", "theta_9"],
		],
	},
	constructors=[
		[["kin::Kinematics", "kin"]],
	]
)
]]]*/
/*[[[end]]]*/
/// \}

